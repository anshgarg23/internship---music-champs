#program to wish me good morning, good afternoon, good evening, good night
import time
current_hour = time.localtime().tm_hour
if current_hour < 12:
    print("good morning")
elif 12 < current_hour < 17:
    print("good afternoon")
elif 17 < current_hour < 22:
     print("good evening")
else:
     print("good night") 
print("program complete")

